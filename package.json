{
  "name": "markmap-vscode",
  "displayName": "Markmap",
  "description": "Visualize your markdown in VSCode",
  "version": "0.0.1",
  "publisher": "gera2ld",
  "icon": "assets/favicon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:markmap-vscode.markmap"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "markmap-vscode.markmap",
        "displayName": "Markmap",
        "selector": [
          {
            "filenamePattern": "*.mm.md"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "transform": "locky yarn yarn"
  },
  "devDependencies": {
    "@gera2ld/locky": "^0.1.0",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.162",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.2",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "mocha": "^8.2.0",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "markmap-lib": "^0.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn transform -t",
      "pre-push": "yarn lint"
    }
  },
  "repository": "https://github.com/gera2ld/markmap-vscode",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT"
}
