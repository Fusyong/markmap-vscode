{
  "name": "markmap-vscode",
  "version": "0.0.5",
  "description": "Visualize your markdown in VSCode",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "displayName": "Markmap",
  "publisher": "gera2ld",
  "icon": "assets/favicon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "markmap",
    "mindmap",
    "markdown"
  ],
  "activationEvents": [
    "onCustomEditor:markmap-vscode.markmap"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "markmap-vscode.markmap",
        "displayName": "Markmap",
        "selector": [
          {
            "filenamePattern": "*.mm.md"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn lint && yarn clean && yarn build",
    "build": "rollup -c rollup.conf.js",
    "lint": "eslint --ext .ts,.tsx .",
    "watch": "rollup -wc rollup.conf.js",
    "transform": "locky yarn yarn",
    "clean": "del dist types"
  },
  "repository": "https://github.com/gera2ld/markmap-vscode",
  "main": "dist/extension.js",
  "files": [
    "dist",
    "types"
  ],
  "typings": "types/index.d.ts",
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@gera2ld/locky": "^0.1.0",
    "@gera2ld/plaid": "~2.1.3",
    "@gera2ld/plaid-common-ts": "~2.1.5",
    "@gera2ld/plaid-rollup": "~2.1.3",
    "@types/glob": "^7.1.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.6",
    "@types/vscode": "^1.50.0",
    "del-cli": "^3.0.1",
    "husky": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "markmap-lib": "^0.10.1",
    "markmap-toolbar": "^0.1.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "yarn transform -t"
    }
  }
}
